.news-label-cell{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:horizontal;-webkit-box-direction:normal;-webkit-flex-direction:row;flex-direction:row;margin:0 30rpx;padding:32rpx 0;-webkit-box-align:center;-webkit-align-items:center;align-items:center;border-bottom:1rpx solid hsla(0,0%,86.7%,.6)}.news-label-cell .label-cell-detail{-webkit-box-flex:1;-webkit-flex:1;flex:1}.news-label-cell .label-cell-detail .cell-title{color:#222;line-height:42rpx;font-size:34rpx;-webkit-line-clamp:3;overflow:hidden;text-overflow:ellipsis;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical}.news-label-cell .label-cell-detail .list-img{margin-top:12rpx;display:-webkit-box;display:-webkit-flex;display:flex}.news-label-cell .label-cell-detail .list-img .list-img-wrap{display:inline-block;width:230rpx;overflow:hidden;height:170rpx;padding-right:4rpx}.news-label-cell .label-cell-detail .list-img .list-img-wrap image{width:100%;height:100%}.news-label-cell .label-cell-detail .cell-info{margin-top:12rpx;color:#999;overflow:hidden;font-size:22rpx}.news-label-cell .label-cell-detail .cell-info .space{margin-right:10rpx}.news-label-cell .label-cell-detail .cell-info .stick-label{border-radius:4rpx;color:#f85959;display:inline-block;text-align:center;border:1rpx solid rgba(248,89,89,.5);padding:2rpx;height:28rpx}.news-label-cell .img-holder{width:230rpx;display:inline-block;vertical-align:middle;height:170rpx;margin-left:24rpx}.news-label-cell .img-holder image{width:100%;height:100%}