<view class="news-label-cell"><view data-event-opts="{{[['tap',[['$emit',['cellTap','$0'],['newsItem.article_url']]]]]}}" class="label-cell-detail" bindtap="__e"><view class="cell-title">{{''+newsItem.title+''}}</view><block wx:if="{{newsItem.image_list&&newsItem.image_list.length>0}}"><view class="list-img"><block wx:for="{{newsItem.image_list}}" wx:for-item="img" wx:for-index="index" wx:key="index"><view class="list-img-wrap"><image src="{{img.url}}"></image></view></block></view></block><view class="cell-info"><block wx:if="{{newsItem.label}}"><text class="stick-label space">{{newsItem.label}}</text></block><block wx:if="{{newsItem.source}}"><text class="src space">{{newsItem.source}}</text></block><block wx:if="{{newsItem.comment_count}}"><text class="comment space">{{newsItem.comment_count}}</text></block><block wx:if="{{newsItem.datetime}}"><text class="time space">{{newsItem.datetime}}</text></block></view></view><block wx:if="{{newsItem.image_url}}"><view class="img-holder"><image src="{{newsItem.image_url}}"></image></view></block></view>